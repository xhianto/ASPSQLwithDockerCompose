@model FotoUploadViewModel
@{
    ViewData["Title"] = "Uploaden";
}

<h2>Foto uploaden</h2>
<form asp-action="FotoUpload" method="post" enctype="multipart/form-data">
    <input asp-for="PlantNr" type="hidden" />
    <input type="file" name="Foto" />
    <input type="submit" value="Uploaden" />
</form>

